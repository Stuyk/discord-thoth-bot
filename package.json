{
  "name": "discord-thoth",
  "version": "2.0.0",
  "description": "Auto threader discord bot.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "npm run build && node ./dist/index.js",
    "rebuild": "rmdir /s /q dist && npm run build && node ./dist/index.js",
    "reinstall": "rmdir /s /q node_modules && npm install"
  },
  "author": "stuyk",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^13.7.0",
    "dotenv": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "typescript": "^4.5.4"
  },
  "prettier": {
    "tabWidth": 4,
    "printWidth": 120,
    "semi": true,
    "arrowParens": "always",
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0"
  }
}
